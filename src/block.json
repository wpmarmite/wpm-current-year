
{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telex/current-year",
	"version": "0.1.0",
	"title": "Current Year",
	"category": "text",
	"icon": "calendar-alt",
	"description": "Display the current year dynamically with customizable formatting.",
	"example": {
		"attributes": {
			"prefix": "Copyright Â© ",
			"suffix": ""
		}
	},
	"attributes": {
		"prefix": {
			"type": "string",
			"default": ""
		},
		"suffix": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"align": true,
		"alignWide": false
	},
	"textdomain": "wpm-current-year",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
